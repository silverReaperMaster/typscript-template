{
    "name": "typescript-template",
    "version": "0.0.1",
    "description": "template for typescript ",
    "main": "index.js",
    "node": ">=16.0.0",
    "scripts": {
        "test": "vitest",
        "coverage": "cross-env FORCE_COLOR=0 vitest run --coverage",
        "test:ui": "npx vitest --ui",
        "dev": "cross-env NODE_OPTIONS=--trace-deprecation ts-node ./src/ index.ts",
        "start": "",
        "lint": "eslint .",
        "check-rules": "npx eslint-config-prettier ./src/index.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/silverReaperMaster/typscript-template.git"
    },
    "author": "Kevin Genskowsky",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/silverReaperMaster/typscript-template/issues"
    },
    "homepage": "https://github.com/silverReaperMaster/typscript-template#readme",
    "devDependencies": {
        "@types/node": "^16.11.45",
        "@types/triple-beam": "^1.3.2",
        "@typescript-eslint/eslint-plugin": "^5.28.0",
        "@vitest/ui": "^0.15.1",
        "c8": "^7.11.3",
        "cross-env": "^7.0.3",
        "eslint": "^8.17.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-no-loops": "^0.3.0",
        "pre-commit": "^1.2.2",
        "prettier": "2.7.0",
        "ts-node": "^10.8.1",
        "typescript": "^4.0.3",
        "vitest": "^0.15.0"
    },
    "dependencies": {
        "chalk": "^5.0.1",
        "winston": "^3.7.2",
        "zod": "^3.17.3"
    },
    "pre-commit": {
        "run": [
            "coverage"
        ],
        "silent": true,
        "colors": false
    }
}
